{
  "name": "typescript-starter",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Typescript starter with vitest, prettier, eslint. All the tools you need to build your project.",
  "main": "index.ts",
  "repository": "git@github.com:yazaldefilimonepinto/typescript-starter.git",
  "author": "Yazalde Filimone <yazaldefilimon@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node  ./dist/index.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "dev:watch": "tsx watch ./src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write './**/*.ts'",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:all": " npm run format && npm run lint:fix && npm run lint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.2",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-react-jsx": "^7.22.15",
    "@babel/types": "^7.23.0",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "lodash": "^4.17.21",
    "vite": "^4.4.11"
  }
}
